package week.day1;

public class Fibonacciseries {
	public static void main(String[] args) {
	int  range,firstNum,secNum,sum;
	range=8;
	firstNum=0;
	secNum=1;
	System.out.println(firstNum);
	System.out.println(secNum);
	for (int i = 1; i <=range; i++) {
		sum=firstNum+secNum;
		firstNum=secNum;
		secNum=sum;
		System.out.println(sum);
	}
	}
}
